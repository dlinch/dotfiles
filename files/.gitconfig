# This is Git's per-user configuration file.
[credential]
  helper = osxkeychain
[user]
  name = Derik Linch
  email = dlinch33@gmail.com
[color]
  ui = true
  pager = true
[color "status"]
  added = green
  changed = yellow
  untracked = cyan
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = 227
  commit = 227 bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "diff-highlight"]
  oldNormal = red bold
  oldHighlight = red bold 52
  newNormal = green bold
  newHighlight = green bold 22
[pull]
  rebase = true
[fetch]
  prune = true
  pruneTags = true
[push]
  default = current
  followTags = true
  useForceIfIncludes = true
  autoSetupRemote = true
[branch]
  autoSetupRebase = always
[rebase]
  abbreviateCommands = true
  autoSquash = true
  autoStash = true
[rerere]
  enabled = true
  autoUpdate = true
